					<div class="checkbox checkbox check-success"> <a href="#" data-toggle="modal" data-target="#myModal">Esqueceu sua senha?</a>&nbsp;&nbsp;
					  <input type="checkbox" id="checkbox1" value="1">
					  <label for="checkbox1">Manter Conectado </label>
					</div>
				  </div>
				  <input type="submit" class="btn btn-primary btn-cons" id="login_toggle" value="Logar" style="float:right;">
				  </div>
			  </form>
			<form id="frm_register" class="animated fadeIn" method="POST" action="" style="display:none">
                    <div class="row form-row m-l-20 m-r-20 xs-m-l-10 xs-m-r-10">
                      <div class="col-md-6 col-sm-6">
                        <input name="nome" id="reg_username" type="text"  class="form-control" placeholder="Nome">
                      </div>
                      <div class="col-md-6 col-sm-6">
                        <input name="sobre_nome" id="reg_pass" type="text"  class="form-control" placeholder="Sobrenome">
                      </div>
                    </div>	
                    <div class="row form-row m-l-20 m-r-20 xs-m-l-10 xs-m-r-10">
                      <div class="col-md-12">
                        <input name="email" id="reg_mail" type="text"  class="form-control" placeholder="E-Mail">
                      </div>
                    </div>	
                    <div class="row form-row m-l-20 m-r-20 xs-m-l-10 xs-m-r-10">
                      <div class="col-md-6 col-sm-6">
                        <input name="senha" id="reg_first_Name" type="password"  class="form-control" placeholder="Senha">
                      </div>
                      <div class="col-md-6 col-sm-6">
                        <input name="rep_senha" id="reg_first_Name" type="password"  class="form-control" placeholder="Repita sua senha">
                      </div>
                    </div>	
                    <div class="row form-row m-l-20 m-r-20 xs-m-l-10 xs-m-r-10">
                      <div class="col-md-12 ">
                        <input type="submit" class="btn btn-primary btn-cons" id="login_toggle" value="Cadastrar" style="float:right;">
