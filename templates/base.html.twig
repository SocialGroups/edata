                  <img src="{{notificacaoPendente.foto_perfil|raw}}" alt="" data-src="{{notificacaoPendente.foto_perfil|raw}}" data-src-retina="{{notificacaoPendente.foto_perfil|raw}}" width="35" height="35">
                </div>

                {% if notificacaoPendente.tipo|raw == 'comentario' %}

                <div class="message-wrapper">
                  <div class="heading" style="color:#FFF">Comentou</div>
                  <div class="description">{{notificacaoPendente.conteudo|raw}}</div>
                  <div class="date pull-left" style="color:#FFF;">{{notificacaoPendente.dataHora|date("d/m/Y H:i") }}</div>                
                </div>  

                {% elseif notificacaoPendente.tipo|raw == 'convite' %}

                <div class="message-wrapper">
                  <div class="heading" style="color:#FFF">Convidou você para o Grupo :</div>
                  <div class="description">{{notificacaoPendente.grupoNome|raw}}</div>
                  <div class="date pull-left" style="color:#FFF;">{{notificacaoPendente.grupoDataHora|date("d/m/Y H:i") }}</div>                
                </div>  

                {% else %}

                <div class="message-wrapper">
                  <div class="heading" style="color:#FFF">Compartilhou</div>
                  <div class="description">{{notificacaoPendente.descricao|striptags|slice(0, 80)}}...</div>
                  <div class="date pull-left" style="color:#FFF;">{{notificacaoPendente.BoxDataHora|date("d/m/Y H:i") }}</div>                   
                </div>

                {% endif %}

                <div class="clearfix"></div>                  
              </div>
