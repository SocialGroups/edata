              <a href="/grupos/get/{{notificacaoPendente.value|raw}}">

            {% else %}

             <a href="/notificacao-pendente-completa/{{notificacaoPendente.value|raw}}/{{notificacaoPendente.id|raw}}" class="exibirNotificacaoPendente">

            {% endif %}
