{% block navigationbar %}					
	<div class="breadcrumbs" id="breadcrumbs">
		<script type="text/javascript">
			try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
		</script>

		<ul class="breadcrumb">
			<li>
				<i class="icon-home home-icon"></i>
				<a href="/">Home</a>
			</li>
			<li class="active">{% block navigationtitle %}Dashboard{% endblock %}</li>
		</ul><!-- .breadcrumb -->
		
		<div class="nav-search" id="nav-search">
			{% block buttonnew %}{% endblock %}
		</div><!-- #nav-search -->
	</div>
{% endblock %}